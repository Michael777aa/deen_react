{
  "expo": {
    "name": "modernwash",
    "slug": "modernwash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "modernwash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.odette.carwash",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby prayer times.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to provide accurate reminders even in the background.",
             "NSCameraUsageDescription": "We need access to your camera to scan products."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.odette.carwash"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dddc2bc3-855a-43e9-a63b-4697894eb276"
      }
    },
    "owner": "mikee27a"
  }
}
